{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": { "persistent": true, "cache": false },
    "lint": { "outputs": [] },
    "lint:fix": { "outputs": [] },
    "ts:check": { "outputs": [] },
    "stylelint": { "outputs": [] },
    "check": { "outputs": [] },
    "build": {
      "dependsOn": ["^build:lib"],
      "outputs": ["dist/**"]
    }
  }
}
